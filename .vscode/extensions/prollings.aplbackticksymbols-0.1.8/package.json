{
	"name": "aplbackticksymbols",
	"displayName": "APL Backtick Symbols",
	"description": "APL symbols from backtick codes",
	"version": "0.1.8",
	"publisher": "prollings",
	"icon": "icon.png",
	"markdown": "standard",
	"keywords": [
		"apl",
		"dyalog"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/prollings/apl_backtick_symbols"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onLanguage:apl"
	],
	"contributes": {
		"keybindings": {
			"command": "extension.backtick",
			"key": "`",
			"when": "editorLangId == apl && editorTextFocus"
		},
		"languages": [
			{
				"extensions": [
					".apl"
				],
				"id": "apl"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"__metadata": {
		"id": "41dddbf5-ff57-4df5-b962-48c647634e91",
		"publisherId": "51d60294-300a-48da-90e7-11f75c055fa3",
		"publisherDisplayName": "prollings",
		"targetPlatform": "undefined",
		"updated": false,
		"isPreReleaseVersion": false,
		"preRelease": false,
		"installedTimestamp": 1656933393770
	}
}